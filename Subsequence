import java.util.ArrayList;
public class Stringque {
    public static void main(String[] args) {
        String str=new String("sakshi");
        ArrayList<StringBuilder> list2D= new ArrayList<StringBuilder>();
        for(int i=0;i<str.length();i++){
            for(int l=i;l<str.length();l++){
                StringBuilder subsquence=new StringBuilder();
                for(int j=i;j<=l;j++){
                    subsquence.append(str.charAt(j));
                }
                list2D.add(subsquence);
            }
        }
        System.out.println(list2D);
    }
}
 
