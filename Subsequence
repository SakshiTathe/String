
import java.util.ArrayList;
import java.util.List;
public class Stringque {

    public static void main(String[] args) {
        String str=new String("sakshi");

        ArrayList<List<Character>> list2D= new ArrayList<List<Character>>();

        for(int i=0;i<str.length();i++){
            for(int l=i;l<str.length();l++){
                List<Character> subsquence=new ArrayList<>();
                for(int j=i;j<=l;j++){
                    subsquence.add(str.charAt(j));
                }
                list2D.add(subsquence);
            }
        }
        System.out.println(list2D);
    }
}
 
